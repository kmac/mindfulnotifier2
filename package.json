{
  "name": "mindfulnotifier",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchall": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "prebuild": "expo prebuild --clean",
    "export": "expo export --platform web --clear",
    "dist": "npm run prebuild && npm run export",
    "lint": "expo lint",
    "android-development": "eas build --platform android --profile development --local",
    "android-preview": "eas build --platform android --profile preview --local",
    "android-production": "eas build --platform android --profile production --local"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg))"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^7.1.19",
    "@reduxjs/toolkit": "^2.9.2",
    "expo": "^54.0.21",
    "expo-audio": "^1.0.14",
    "expo-av": "^16.0.7",
    "expo-background-task": "^1.0.8",
    "expo-constants": "^18.0.10",
    "expo-device": "^8.0.9",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.17",
    "expo-font": "^14.0.9",
    "expo-intent-launcher": "~13.0.7",
    "expo-linking": "^8.0.8",
    "expo-notifications": "^0.32.12",
    "expo-router": "^6.0.14",
    "expo-sharing": "~14.0.7",
    "expo-splash-screen": "^31.0.10",
    "expo-status-bar": "^3.0.8",
    "expo-system-ui": "^6.0.8",
    "expo-task-manager": "^14.0.8",
    "expo-web-browser": "^15.0.8",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-native": "^0.82.1",
    "react-native-gesture-handler": "^2.29.0",
    "react-native-paper": "^5.14.5",
    "react-native-paper-dates": "^0.22.50",
    "react-native-screens": "^4.18.0",
    "react-native-web": "^0.21.2",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "sweetalert2": "^11.26.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.2",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-jest": "^30.2.0",
    "eslint": "^9.38.0",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.13",
    "react-test-renderer": "^19.2.0",
    "typescript": "^5.9.3"
  },
  "private": true
}
