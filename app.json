{
  "expo": {
    "name": "Mindful Notifier",
    "slug": "mindfulnotifier",
    "version": "0.1.9",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mindfulnotifier",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mindfulnotifier.app"
    },
    "android": {
      "versionCode": 7,
      "package": "com.mindfulnotifier.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "POST_NOTIFICATIONS",
        "SCHEDULE_EXACT_ALARM",
        "USE_EXACT_ALARM",
        "WAKE_LOCK",
        "VIBRATE",
        "REQUEST_IGNORE_BATTERY_OPTIMIZATIONS",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_SPECIAL_USE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "buildNumber": "1"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/sounds/bell_inside.mp3",
            "./assets/sounds/bowl_struck.mp3",
            "./assets/sounds/ding_soft.mp3",
            "./assets/sounds/tibetan_bell_ding_b.mp3",
            "./assets/sounds/zenbell_1.mp3"
          ],
          "androidMode": "default",
          "androidCollapsedTitle": "Mindful Notifier"
        }
      ],
      [
        "expo-task-manager",
        {
          "android": {
            "enableBackgroundTasks": true
          }
        }
      ],
      "expo-background-task",
      "./plugins/withRawResourceKeep.js",
      "./plugins/withForegroundService.js",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCanary": true,
      "appRoot": "src"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "605b0bba-69d9-4f70-8eca-b5c86d521255"
      }
    }
  }
}
